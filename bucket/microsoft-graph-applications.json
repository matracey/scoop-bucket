{
    "version": "2.26.1",
    "description": "Microsoft Graph PowerShell Cmdlets",
    "homepage": "https://github.com/microsoftgraph/msgraph-sdk-powershell",
    "license": "https://aka.ms/devservicesagreement",
    "depends": "microsoft-graph-authentication",
    "pre_install": "Remove-Item \"$dir\\_rels\", \"$dir\\package\", \"$dir\\*Content_Types*.xml\" -Recurse",
    "pre_uninstall": [
        "$InstalledDeps = $manifest.depends | Where-Object { Test-Path \"$(appdir $_)\\current\" }",
        "if (-not $InstalledDeps) { return }",
        "if ($global) { scoop uninstall --global $InstalledDeps }",
        "else { scoop uninstall $InstalledDeps }"
    ],
    "checkver": {
        "regex": "<h2>([\\d\\.]+)</h2>",
        "url": "https://www.powershellgallery.com/packages/microsoft.graph.applications"
    },
    "autoupdate": {
        "url": "https://www.powershellgallery.com/api/v2/package/microsoft.graph.applications/$version#/microsoft.graph.applications.nupkg"
    },
    "hash": "09de584f91b747859b1c6d2d9bd37dc767c74f6265bb61dfff73f0be8018a1b3",
    "url": "https://www.powershellgallery.com/api/v2/package/microsoft.graph.applications/2.26.1#/microsoft.graph.applications.nupkg",
