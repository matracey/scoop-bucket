{
    "version": "2.27.0",
    "description": "Microsoft Graph PowerShell Cmdlets",
    "homepage": "https://github.com/microsoftgraph/msgraph-sdk-powershell",
    "license": "https://aka.ms/devservicesagreement",
    "depends": "microsoft-graph-authentication",
    "pre_install": "Remove-Item \"$dir\\_rels\", \"$dir\\package\", \"$dir\\*Content_Types*.xml\" -Recurse",
    "pre_uninstall": [
        "$InstalledDeps = $manifest.depends | Where-Object { Test-Path \"$(appdir $_)\\current\" }",
        "if (-not $InstalledDeps) { return }",
        "if ($global) { scoop uninstall --global $InstalledDeps }",
        "else { scoop uninstall $InstalledDeps }"
    ],
    "checkver": {
        "regex": "<h2>([\\d\\.]+)</h2>",
        "url": "https://www.powershellgallery.com/packages/microsoft.graph.compliance"
    },
    "autoupdate": {
        "url": "https://www.powershellgallery.com/api/v2/package/microsoft.graph.compliance/$version#/microsoft.graph.compliance.nupkg"
    },
    "hash": "96efa74b1fa5de8a8a95439cbd87602e6e93be041c53c0a912eb52b3b483bfed",
    "url": "https://www.powershellgallery.com/api/v2/package/microsoft.graph.compliance/2.27.0#/microsoft.graph.compliance.nupkg",
    "psmodule": {
