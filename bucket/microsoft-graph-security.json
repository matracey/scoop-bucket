{
    "version": "2.28.0",
    "description": "Microsoft Graph PowerShell Cmdlets",
    "homepage": "https://github.com/microsoftgraph/msgraph-sdk-powershell",
    "license": "https://aka.ms/devservicesagreement",
    "depends": "microsoft-graph-authentication",
    "pre_install": "Remove-Item \"$dir\\_rels\", \"$dir\\package\", \"$dir\\*Content_Types*.xml\" -Recurse",
    "pre_uninstall": [
        "$InstalledDeps = $manifest.depends | Where-Object { Test-Path \"$(appdir $_)\\current\" }",
        "if (-not $InstalledDeps) { return }",
        "if ($global) { scoop uninstall --global $InstalledDeps }",
        "else { scoop uninstall $InstalledDeps }"
    ],
    "checkver": {
        "regex": "<h2>([\\d\\.]+)</h2>",
        "url": "https://www.powershellgallery.com/packages/microsoft.graph.security"
    },
    "autoupdate": {
        "url": "https://www.powershellgallery.com/api/v2/package/microsoft.graph.security/$version#/microsoft.graph.security.nupkg"
    },
    "hash": "4cbf639ef2dbf73671c759dff8ca37ff057ff4ff8ccfbf2837d306214dbae962",
    "url": "https://www.powershellgallery.com/api/v2/package/microsoft.graph.security/2.28.0#/microsoft.graph.security.nupkg",
    "psmodule": {
        "name": "Microsoft.Graph.Security"
    }
}
